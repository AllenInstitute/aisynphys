#!/bin/bash

cd ~
git clone https://github.com/AllenInstitute/neuroanalysis
cd neuroanalysis
python setup.py develop

cd ~
git clone https://github.com/AllenInstitute/aisynphys --branch=current-release --recursive
cd aisynphys
python setup.py develop

python util/database.py --db-version=synphys_r1.0_2019-08-29_small.sqlite
